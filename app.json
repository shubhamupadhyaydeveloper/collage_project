{
  "expo": {
    "name": "Quizkr",
    "slug": "Quizkr",
    "version": "1.0.0",
    "scheme": "myapp",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shubham-upadhyay.Quizkr"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "androidNavigationBar": {
        "barStyle": "dark-content",
        "backgroundColor": "#222222"
      },
      "package": "com.shubham_upadhyay.Quizkr"
    },
    "extra": {
      "eas": {
        "projectId": "c5383d7c-e94d-4b04-90fd-3c5e26c53bd3"
      }
    },
    "owner": "shubham_upadhyay"
  }
}
